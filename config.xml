<?xml version='1.0' encoding='utf-8'?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.totall.order" version="1.1.1">
  <name>Totall Order</name>
  <description>Aplicativo Totall Order para smart devices.</description>
  <author href="http://www.totall.com.br" email="comercial@totall.com.br">Totall Sistemas</author>
  <content src="index.html"/>
  <icon src="icon.png"/>
  <icon src="www/res/icon/android/36.png" gap:platform="android" gap:qualifier="ldpi"/>
  <icon src="www/res/icon/android/48.png" gap:platform="android" gap:qualifier="mdpi"/>
  <icon src="www/res/icon/android/72.png" gap:platform="android" gap:qualifier="hdpi"/>
  <icon src="www/res/icon/android/96.png" gap:platform="android" gap:qualifier="xhdpi"/>
  <icon src="www/res/icon/blackberry/80.png" gap:platform="blackberry"/>
  <icon src="www/res/icon/blackberry/80.png" gap:platform="blackberry" gap:state="hover"/>
  <icon src="www/res/icon/ios/57.png" gap:platform="ios" width="57" height="57"/>
  <icon src="www/res/icon/ios/72.png" gap:platform="ios" width="72" height="72"/>
  <icon src="www/res/icon/ios/114.png" gap:platform="ios" width="114" height="114"/>
  <icon src="www/res/icon/ios/144.png" gap:platform="ios" width="144" height="144"/>
  <icon src="www/res/icon/webos/64.png" gap:platform="webos"/>
  <icon src="www/res/icon/windows-phone/48.png" gap:platform="winphone"/>
  <icon src="www/res/icon/windows-phone/173.png" gap:platform="winphone" gap:role="background"/>
  <gap:splash src="www/res/screen/android/screen-ldpi-portrait.png" gap:platform="android" gap:qualifier="port-ldpi"/>
  <gap:splash src="www/res/screen/android/screen-mdpi-portrait.png" gap:platform="android" gap:qualifier="port-mdpi"/>
  <gap:splash src="www/res/screen/android/screen-hdpi-portrait.png" gap:platform="android" gap:qualifier="port-hdpi"/>
  <gap:splash src="www/res/screen/android/screen-xhdpi-portrait.png" gap:platform="android" gap:qualifier="port-xhdpi"/>
  <gap:splash src="www/res/screen/blackberry/screen-225.png" gap:platform="blackberry"/>
  <gap:splash src="www/res/screen/ios/screen-iphone-portrait.png" gap:platform="ios" width="320" height="480"/>
  <gap:splash src="www/res/screen/ios/screen-iphone-portrait-2x.png" gap:platform="ios" width="640" height="960"/>
  <gap:splash src="www/res/screen/ios/screen-iphone-portrait-568h-2x.png" gap:platform="ios" width="640" height="1136"/>
  <gap:splash src="www/res/screen/ios/screen-ipad-portrait.png" gap:platform="ios" width="768" height="1024"/>
  <gap:splash src="www/res/screen/ios/screen-ipad-landscape.png" gap:platform="ios" width="1024" height="768"/>
  <gap:splash src="www/res/screen/windows-phone/screen-portrait.jpg" gap:platform="winphone"/>
  <access origin="*"/>
  <plugin name="cordova-plugin-whitelist" version="1"/>
  <allow-navigation href="*" />
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>
